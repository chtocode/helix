{
  "name": "helix-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 4001",
    "build": "next build",
    "start": "next start",
    "stylelint": "stylelint \"styles/**/*.scss\" --config stylelint.config.js",
    "eslint": "eslint ./components ./pages ./utils ./hooks ./config ./model -c .eslintrc.js --fix",
    "i18next:scanner": "i18next-scanner --config i18next-scanner.config.js",
    "lint": "npm run eslint && npm run stylelint && next lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "format": "prettier ./pages/**/*.tsx ./components/**/*.tsx ./config/**/*.ts  ./model/**/*.ts ./utils/**/*.ts --write"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@darwinia/types": "^2.7.3",
    "@fontsource/ibm-plex-sans": "^4.5.7",
    "@metamask/jazzicon": "^2.0.0",
    "@polkadot/api": "^7.15.1",
    "antd": "4.16.1",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.0",
    "camelcase-keys": "^7.0.2",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.6",
    "graphql": "^16.3.0",
    "graphql-hooks": "^5.2.0",
    "graphql-request": "^4.2.0",
    "highcharts": "^10.0.0",
    "highcharts-react-official": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "12.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.0.0",
    "next-plugin-antd-less": "^1.8.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-virtual": "^2.10.4",
    "rxjs": "^7.2.0",
    "store": "^2.0.12",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-angular": "^12.1.4",
    "@commitlint/config-conventional": "^12.0.1",
    "@darwinia/lints": "^1.0.1",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/bn.js": "^5.1.0",
    "@types/lodash": "^4.14.181",
    "@types/node": "17.0.23",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "autoprefixer": "^10.4.4",
    "babel-plugin-import": "^1.13.3",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "7.27.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "i18next-scanner": "^3.1.0",
    "i18next-scanner-typescript": "^1.0.6",
    "less-bundle-promise": "1.0.7",
    "less-plugin-npm-import": "2.1.0",
    "prettier": "^2.2.1",
    "prettier-quick": "0.0.5",
